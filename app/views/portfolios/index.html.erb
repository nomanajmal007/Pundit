<h1>Portfolio Items</h1>


<% @portfolio.each do |portfolio_item| %>
    <p><%= portfolio_item.title%></p>
    <p><%= portfolio_item.subtitle%></p>
    <p><%= portfolio_item.body%></p>
    <%= image_tag portfolio_item.main_image if !portfolio_item.main_image.nil? %>
    <%= image_tag portfolio_item.thumg_image unless portfolio_item.thumg_image.nil? %>
<%end%>